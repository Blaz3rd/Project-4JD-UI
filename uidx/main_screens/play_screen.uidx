{
  "namespace": "play_deluxe",

  /*///////////////////////////////////////////////////////////////////////////
  //  Grid.
  ///////////////////////////////////////////////////////////////////////////*/

  "world_item_grid_base": {
    "type": "grid",
    "size": [ "100%", "default" ],
    "layer": 1,
    "anchor_to": "top_left",
    "anchor_from": "top_left"
  },

  "realms_world_item_grid@play_deluxe.world_item_grid_base": {
    "grid_item_template": "$grid_item_layout",
    "grid_dimension_binding": "$grid_name",
    "collection_name": "$realms_worlds_collection_name",
    "bindings": [
      {
        "binding_name": "$grid_name"
      },
      {
        "binding_name": "#realms_grids_visible",
        "binding_name_override": "#visible"
      }
    ]
  },

  "network_world_item_grid@play_deluxe.world_item_grid_base": {
    "grid_item_template": "play_deluxe.network_world_item",
    "grid_dimension_binding": "$network_world_item_grid_dimension",
    "collection_name": "$collection_name",
    "bindings": [
      {
        "binding_name": "$network_world_item_grid_dimension"
      }
    ]
  },

  "local_world_item_grid@play_deluxe.world_item_grid_base": {
    "grid_item_template": "play_deluxe.local_world_item",
    "grid_dimension_binding": "#local_world_item_grid_dimension",
    "collection_name": "local_worlds",
    "bindings": [
      {
        "binding_name": "#local_world_item_grid_dimension"
      }
    ]
  },

  "legacy_world_item_grid@play_deluxe.world_item_grid_base": {
    "grid_item_template": "play.legacy_world_item",
    "grid_dimension_binding": "#legacy_world_item_grid_dimension",
    "collection_name": "legacy_worlds",
    "bindings": [
      {
        "binding_name": "#legacy_world_item_grid_dimension"
      }
    ]
  },

  "third_party_featured_server_grid": {
    "type": "grid",
    "$more_servers": false,
    "size": [ "100%", "default" ],
    "layer": 1,
    "grid_item_template": "play_deluxe.featured_server_world_item",
    "grid_dimension_binding": "#third_party_featured_item_grid_dimension",
    "collection_name": "third_party_server_network_worlds",
    "$collection_name": "third_party_server_network_worlds",
    "anchor_to": "top_left",
    "anchor_from": "top_left",
    "bindings": [
      {
        "binding_name": "#third_party_featured_item_grid_dimension"
      },
      {
        "binding_name": "#featured_servers_visible",
        "binding_name_override": "#visible"
      }
    ]
  },

  "more_servers_grid": {
    "type": "grid",
    "$more_servers": true,
    "size": [ "100%", "default" ],
    "layer": 1,
    "grid_item_template": "play_deluxe.more_servers_world_item",
    "grid_dimension_binding": "#servers_network_world_item_grid_dimension",
    "collection_name": "servers_network_worlds",
    "$collection_name": "servers_network_worlds",
    "anchor_to": "top_left",
    "anchor_from": "top_left",
    "bindings": [
      {
        "binding_name": "#servers_network_world_item_grid_dimension"
      }
    ]
  },

  /*///////////////////////////////////////////////////////////////////////////
  //  Scroll Panes.
  ///////////////////////////////////////////////////////////////////////////*/

  "scroll_stack_panel_base": {
    "type": "stack_panel",
    "orientation": "vertical",
    "size": [ "100%", "100%c" ]
  },

  "worlds_stack_panel@play_deluxe.scroll_stack_panel_base": {
    "controls": [
      {
        "realms_panel": {
          "type": "stack_panel",
          "size": [ "100%", "100%c" ],
          "bindings": [
            {
              "binding_name": "#personal_realms_grid_visible",
              "binding_name_override": "#visible"
            }
          ],
          "controls": [
            {
              "realms_label@play_deluxe.base_label": {
                "size": [ "default", 12 ],
                "ignored": "$world_picker_screen",
                "text": "playscreen.realms",
                "layer": 10,
                "bindings": [
                  {
                    "binding_name": "#realm_label_visible",
                    "binding_name_override": "#visible"
                  }
                ]
              }
            },
            {
              "personal_realms@play_deluxe.realms_world_item_grid": {
                "ignored": "$world_picker_screen",
                "$grid_name": "#personal_realms_grid_dimension",
                "$realms_worlds_collection_name": "personal_realms",
                "$grid_item_layout": "play_deluxe.realms_world_item"
              }
            }
          ]
        }
      },
      {
        "worlds_label@play_deluxe.base_label": {
          "size": [ "default", 12 ],
          "text": "playscreen.worlds",
          "bindings": [
            {
              "binding_name": "#local_worlds_visible",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "grid@play_deluxe.local_world_item_grid": {}
      }
    ]
  },

  "friends_stack_panel@play_deluxe.scroll_stack_panel_base": {
    "controls": [
      {
        "friends_realms_label@play_deluxe.base_label": {
          "size": [ "default", 12 ],
          "text": "playscreen.joinableRealms",
          "bindings": [
            {
              "binding_name": "#friends_realms_visible",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "friends_realms@play_deluxe.realms_world_item_grid": {
          "$grid_name": "#friend_realms_grid_dimension",
          "$realms_worlds_collection_name": "friends_realms",
          "$grid_item_layout": "play_deluxe.realms_world_item"
        }
      },
      {
        "friends_label@play_deluxe.base_label": {
          "size": [ "default", 12 ],
          "text": "networkWorld.joinable_friends_label",
          "bindings": [
            {
              "binding_name": "#friends_grid_visible",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "friends_grid@play_deluxe.network_world_item_grid": {
          "$network_world_item_grid_dimension": "#friends_network_world_item_grid_dimension",
          "$collection_name": "friends_network_worlds",

          "$network_world_icon": "play_deluxe.friends_server_icon",
          "$hide_network_icon":  "$hide_xbox_live_icon"
        }
      },
      {
        "no_friends_grid_message@play_deluxe.base_label": {
          "size": [ "default", 12 ],
          "text": "#no_friends_grid_message_text",
          "variables": [
            {
              "requires": "$nx_os",
              "$label_content_label": "networkWorld.joinable_nintendo_friends_label"
            }
          ],
          "bindings": [
            {
              "binding_name": "#no_friends_grid_message_text"
            },
            {
              "binding_name": "#no_friends_grid_message_visible",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "lan_label@play_deluxe.base_label": {
          "size": [ "default", 12 ],
          "text": "networkWorld.lan_label",
          "bindings": [
            {
              "binding_name": "#lan_grid_visible",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "lan_grid@play_deluxe.network_world_item_grid": {
          "$network_world_item_grid_dimension": "#lan_network_world_item_grid_dimension",
          "$network_world_icon": "play_deluxe.lan_icon",
          "$collection_name": "lan_network_worlds"
        }
      }
    ]
  },

  "lan_icon": {
    "type": "image",
    "texture": "textures/ui_old/icon_lan",
    "size": [ 16, 16 ]
  },

  "friends_server_icon": {
    "type": "image",
    "size": [ 16, 16 ],
    "texture": "textures/ui_old/icon_xbox"
  },

  "server_stack_panel@play_deluxe.scroll_stack_panel_base": {
    "controls": [
      {
        "more_servers_label@play_deluxe.base_label": {
          "size": [ "default", 12 ],
          "text": "thirdPartyWorld.Featured",
          "layer": 1,
          "bindings": [
            {
              "binding_name": "#featured_servers_visible",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "third_party_featured_grid@play_deluxe.third_party_featured_server_grid": { }
      },
      {
        "more_servers_label@play_deluxe.base_label": {
          "size": [ "default", 12 ],
          "text": "networkWorld.more_servers",
          "layer": 1,
          "bindings": [
            {
              "binding_name": "#servers_grid_visible",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "more_servers_grid@play_deluxe.more_servers_grid": { }
      }
    ]
  },

  /*///////////////////////////////////////////////////////////////////////////
  //  Button Panels.
  ///////////////////////////////////////////////////////////////////////////*/

  "local_world_item": {
    "type": "stack_panel",
    "size": [ "100%", 32 ],
    "orientation": "horizontal",
    "controls": [
      { "local_world_item_button@play_deluxe.local_world_item_button": { } },
      { "local_world_edit_button@play_deluxe.local_world_edit_button": { } }
    ]
  },

  "realms_world_item": {
    "type": "stack_panel",
    "size": [ "100%", 32 ],
    "orientation": "horizontal",
    "controls": [
      { "realms_world_item_button@play_deluxe.realms_world_item_button": { } },
      {
        "edit_panel": {
          "type": "panel",
          "size": [ 32, 32 ],
          "controls": [
            { "realms_world_edit_button@play_deluxe.realms_world_edit_button": { "layer": 1 } }
          ],
          "bindings": [
            {
              "binding_name": "#realms_edit_visible",
              "binding_name_override": "#visible",
              "binding_type": "collection",
              "binding_collection_name": "$realms_worlds_collection_name"
            }
          ]
        }
      },
      {
        "feed_panel": {
          "type": "panel",
          "size": [ 32, 32 ],
          "controls": [
            {
              "realms_feed_button@play_deluxe.realms_feed_button": {
                "layer": 1
              }
            }
          ]
        }
      },
      {
        "leave_realm_panel": {
          "type": "panel",
          "size": [ 32, 32 ],
          "controls": [
            { "leave_friends_realm_button@play_deluxe.leave_friends_realm_button": { "layer": 1 } }
          ],
          "bindings": [
            {
              "binding_name": "#leave_realm_button_visible",
              "binding_name_override": "#visible",
              "binding_type": "collection",
              "binding_collection_name": "$realms_worlds_collection_name"
            }
          ]
        }
      }
    ]
  },

  "network_world_item": {
    "type": "stack_panel",
    "size": [ "100%", 32 ],
    "orientation": "horizontal",
    "controls": [
      { "network_world_item_button@play_deluxe.network_world_item_button": { } }
    ]
  },

  "featured_server_world_item": {
    "type": "stack_panel",
    "size": [ "100%", 32 ],
    "orientation": "horizontal",
    "controls": [
      { "more_servers_world_item_button@play_deluxe.more_servers_world_item_button": { } }
    ]
  },

  "more_servers_world_item": {
    "type": "stack_panel",
    "size": [ "100%", 32 ],
    "orientation": "horizontal",
    "controls": [
      { "more_servers_world_item_button@play_deluxe.more_servers_world_item_button": { } },
      { "network_server_world_edit_button@play_deluxe.network_server_world_edit_button": { "ignored": "$ignore_add_servers" } }
    ]
  },

  /* Local World */

  "local_world_lock_panel": {
    "type": "panel",
    "size": [ "100%c", "100%c" ],
    "controls": [
      {
        "lock@ui_old_textures.icon_lock": {
          "color": "$text_color",
          "offset": [ 0, 1 ]
        }
      }
    ],
    "bindings": [
      {
        "binding_name": "#world_lock_visible",
        "binding_type": "collection",
        "binding_name_override": "#visible",
        "binding_collection_name": "local_worlds"
      }
    ]
  },

  "local_world_item_button_content": {
    "type": "stack_panel",
    "orientation": "horizontal",
    "size": [ "100%-10px", "100%-8px" ],
    "layer": 4,
    "$border_texture": "textures/uidx/dark_indent_default",
    "controls": [
      {
        "local_world_screenshot@play.collection_world_screenshot": {
          "$collection_binding_name": "local_worlds",
          "$image_binding_name": "#local_world_image",
          "$texture_source_binding_name": "#local_world_texture_source"
        }
      },
      {
        "padding": {
          "type": "panel",
          "size": [ 5, "100%" ]
        }
      },
      {
        "text_panel": {
          "type": "panel",
          "size": [ "fill", "100%" ],
          "controls": [
            { "local_world_text_panel@play_deluxe.local_world_text_panel": { } }
          ]
        }
      }
    ]
  },

  "local_world_text_panel": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "controls": [
      {
        "top_text": {
          "type": "stack_panel",
          "orientation": "horizontal",
          "size": [ "100%", "50%" ],
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "controls": [
            {
              "local_world_name@play.local_world_name": {
                "shadow": true,
                "size": [ "fill", 9 ],
                "color": "$text_color",
                "layer": 1
              }
            },
            {
              "lock_1@play_deluxe.local_world_lock_panel": {}
            },
            {
              "local_world_filesize@play.local_world_filesize": {
                "shadow": true,
                "size": [ "default", 9 ],
                "color": [ 0.75, 0.75, 0.75 ],
                "layer": 1
              }
            }
          ]
        }
      },
      {
        "bottom_text": {
          "anchor_from": "bottom_left",
          "anchor_to": "bottom_left",
          "type": "stack_panel",
          "orientation": "horizontal",
          "size": [ "100%", "50%" ],
          "controls": [
            {
              "local_world_game_mode@play.local_world_game_mode": {
                "shadow": true,
                "size": [ "default", 9 ],
                "color": [ 0.75, 0.75, 0.75 ],
                "layer": 1
              }
            },
            {
              "padding_0": {
                "type": "panel",
                "size": [ 11, 0 ]
              }
            },
            {
              "local_world_date@play.local_world_date": {
                "shadow": true,
                "size": [ "default", 9 ],
                "color": [ 0.75, 0.75, 0.75 ],
                "layer": 1
              }
            }
          ]
        }
      }
    ]
  },

  /* Realms */

  "realms_world_item_button_content": {
    "type": "stack_panel",
    "orientation": "horizontal",
    "size": [ "100%-10px", "100%-8px" ],
    "layer": 4,
    "$border_texture": "textures/uidx/dark_indent_default",
    "variables": [
      {
        "requires": "($realms_worlds_collection_name = $personal_realm_collection_name)",
        "$show_screenshot": true
      },
      {
        "requires": "($realms_worlds_collection_name = $friend_realm_collection_name)",
        "$show_screenshot": false
      }
    ],
    "controls": [
      {
        "realm_screenshot@play.realm_screenshot": {
          "ignored": "(not $show_screenshot)"
        }
      },
      {
        "padding": {
          "type": "panel",
          "size": [ 5, "100%" ],
          "ignored": "(not $show_screenshot)"
        }
      },
      {
        "left_panel": {
          "type": "panel",
          "size": [ "fill", "100%" ],
          "controls": [
            { "realms_text_panel@play_deluxe.realms_text_panel": { } }
          ]
        }
      },
      {
        "padding_2": {
          "type": "panel",
          "size": [ "100%c", "100%" ],
          "controls": [
            {
              "realms_world_player_count@play.world_list_text": {
                "text": "#realms_world_player_count",
                "color": "$secondary_text_color",
                "size": [ "default", "default" ],
                "shadow": true,
                "bindings": [
                  {
                    "binding_name": "#realms_world_player_count",
                    "binding_type": "collection",
                    "binding_collection_name": "$realms_worlds_collection_name"
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "padding_1": {
          "type": "panel",
          "size": [ 5, "100%" ]
        }
      },
      {
        "padding_2": {
          "type": "panel",
          "size": [ 5, "100%" ],
          "controls": [
            { "realms_world_game_status_icon@play_deluxe.realms_world_game_status_icon": {} }
          ]
        }
      },
      {
        "padding_3": {
          "type": "panel",
          "size": [ 5, "100%" ]
        }
      }
    ]
  },

  "realms_world_game_status_icon": {
    "type": "panel",
    "size": [ 9, 10 ],
    "controls": [
      {
        "game_online_icon@ui_deluxe_textures.dot_1": {
          "bindings": [
            {
              "binding_name": "#realms_game_online",
              "binding_name_override": "#visible",
              "binding_type": "collection",
              "binding_collection_name": "$realms_worlds_collection_name"
            }
          ]
        }
      },
      {
        "game_unavailable_icon@ui_deluxe_textures.dot_2": {
          "bindings": [
            {
              "binding_name": "#realms_game_unavailable",
              "binding_name_override": "#visible",
              "binding_type": "collection",
              "binding_collection_name": "$realms_worlds_collection_name"
            }
          ]
        }
      },
      {
        "game_offline_icon@ui_deluxe_textures.dot_3": {
          "bindings": [
            {
              "binding_name": "#realms_game_offline",
              "binding_name_override": "#visible",
              "binding_type": "collection",
              "binding_collection_name": "$realms_worlds_collection_name"
            }
          ]
        }
      }
    ]
  },

  "realms_text_panel": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "controls": [
      {
        "top_text": {
          "type": "stack_panel",
          "orientation": "horizontal",
          "size": [ "100%", "50%" ],
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "controls": [
            { "realms_world_header@play.realms_world_header": { "shadow": true } }
          ]
        }
      },
      {
        "bottom_text": {
          "anchor_from": "bottom_left",
          "anchor_to": "bottom_left",
          "type": "stack_panel",
          "orientation": "horizontal",
          "size": [ "100%", "50%" ],
          "controls": [
            { "realms_world_details@play.realms_world_details": { "shadow": true } }
          ]
        }
      }
    ]
  },

  /* Network */

  "network_world_item_button_content": {
    "type": "stack_panel",
    "orientation": "horizontal",
    "size": [ "100%", "100%" ],
    "layer": 20,
    "controls": [
      {
        "left_panel": {
          "type": "panel",
          "size": [ "fill", 32 ],
          "controls": [
            { "network_text_panel@play_deluxe.network_text_panel": { } }
          ]
        }
      },
      {
        "padding_2": {
          "type": "panel",
          "size": [ "100%c", 32 ],
          "controls": [
            {
              "world_player_count@play.world_list_text": {
                "text": "#network_world_player_count",
                "color": "$secondary_text_color",
                "size": [ "default", "default" ],
                "shadow": true,
                "bindings": [
                  {
                    "binding_name": "#network_world_player_count",
                    "binding_type": "collection",
                    "binding_collection_name": "$collection_name"
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "padding_1": {
          "type": "panel",
          "size": [ 5, 0 ]
        }
      },
      {
        "padding_2": {
          "type": "panel",
          "size": [ 5, 32 ],
          "controls": [
            {
              "network_world_game_status_icon@play_deluxe.network_world_game_status_icon": {
                "layer": 1
              }
            }
          ]
        }
      },
      {
        "padding_3": {
          "type": "panel",
          "size": [ 5, 0 ]
        }
      },
      {
        "network_world_type_icon@play.network_world_type_icon": {
          "layer": 4
        }
      },
      {
        "padding_3": {
          "type": "panel",
          "size": [ 5, 0 ]
        }
      }
    ]
  },

  "network_world_game_status_icon": {
    "type": "panel",
    "size": [ "100%y", "100%" ],
    "controls": [
      {
        "game_online_icon@ui_deluxe_textures.dot_1": {
          "bindings": [
            {
              "binding_name": "#game_online",
              "binding_name_override": "#visible",
              "binding_type": "collection",
              "binding_collection_name": "$collection_name"
            }
          ]
        }
      },
      {
        "game_unavailable_icon@ui_deluxe_textures.dot_2": {
          "bindings": [
            {
              "binding_name": "#game_unavailable",
              "binding_name_override": "#visible",
              "binding_type": "collection",
              "binding_collection_name": "$collection_name"
            }
          ]
        }
      },
      {
        "game_offline_icon@ui_deluxe_textures.dot_3": {
          "bindings": [
            {
              "binding_name": "#game_offline",
              "binding_name_override": "#visible",
              "binding_type": "collection",
              "binding_collection_name": "$collection_name"
            }
          ]
        }
      }
    ]
  },

  "network_text_panel": {
    "type": "panel",
    "size": [ "100%-10px", "100%-8px" ],
    "controls": [
      {
        "top_text": {
          "type": "stack_panel",
          "orientation": "horizontal",
          "size": [ "100%", "50%" ],
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "controls": [
            {
              "network_world_header_text@play.world_list_text": {
                "text": "#network_world_header",
                "color": "$text_color",
                "shadow": true,
                "bindings": [
                  {
                    "binding_name": "#network_world_header",
                    "binding_type": "collection",
                    "binding_collection_name": "$collection_name"
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "bottom_text": {
          "anchor_from": "bottom_left",
          "anchor_to": "bottom_left",
          "type": "stack_panel",
          "orientation": "horizontal",
          "size": [ "100%", "50%" ],
          "controls": [
            { 
              "network_world_details@play.network_world_details": {
                "shadow": true,
                "bindings": [
                  {
                    "binding_name": "#network_world_details",
                    "binding_type": "collection",
                    "binding_collection_name": "$collection_name"
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },

  /* More Servers */  
